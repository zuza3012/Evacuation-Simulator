<Window x:Class="Ha.ParameterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ha"
        mc:Ignorable="d"
        Title="Set parameters" Height="192" Width="256">
    <Grid Margin="-9,-10,-6.4,-7.6">
        <Label Content="Panic parameter:" HorizontalAlignment="Left" Margin="20,40,0,0" VerticalAlignment="Top" Width="97"/>
        <TextBox x:Name="panicParameterTB" Text="0" HorizontalAlignment="Left" Height="23" Margin="120,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Content="Set" HorizontalAlignment="Left" Margin="86,133,0,0" VerticalAlignment="Top" Width="75" Click="SetParameters"/>

    </Grid>
</Window>
